<script>
function valideate() {
    var username = document.getElementById("username").value;
    var password = document.getElementById("password").value;
    if (username == "abc" && password == "123") {
    alert("logged in");
    window.location.href="cookie.html";
    }
else {
    alert("did not login");

    }
}
</script>


